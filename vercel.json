{
    "version": 2,
    "builds": [
      {
        "src": "server/**/*.py",
        "use": "@vercel/python"
      }
    ],
    "functions": {
      "server/**/*.py": {
        "memory": 128,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/server/(.*)",
        "dest": "/server/$1.py"
      }
    ]
  }
  